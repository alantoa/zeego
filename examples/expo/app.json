{
  "name": "zeeg-example",
  "displayName": "Zeeg Example",
  "expo": {
    "name": "zeeg-example",
    "slug": "zeeg-example",
    "description": "Example app for zeego",
    "privacy": "public",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "scheme": "zeeg-example",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nandorojo.zeeg-example"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.nandorojo.zeegexample"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.8.0",
            "unstable_networkInspector": true
          }
        }
      ]
    ]
  }
}
